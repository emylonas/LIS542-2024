        <header>
            <title>Peanuts</title> by <creator>Charles Schulz</creator>. <date when="1957-05-11">May 11, 1957</date>
        </header>
        <panel n="1">
            <graphic>
                <title>Peanuts</title>
                <character n="Lucy"/>
                <character n="Charlie Brown">
                    <dialogue>PEOPLE COME AND PEOPLE GO, BUT THE EARTH KEEPS ON SPINNING..</dialogue>
                </character>
            </graphic>
        </panel>
        <panel n="2">
            <graphic>
                <character n="Lucy">
                    <dialogue>"SPINNING"? WHAT DO YOU MEAN "SPINNING"?</dialogue>
                </character>
                <character n="Charlie Brown">
                    <dialogue>SPINNING THROUGH SPACE..THE EARTH KEEPS ON SPINNING THROUGH SPACE..</dialogue>
                </character>
                <foreground n="brick wall"/>
            </graphic>
        </panel>
        <panel n="3">
            <graphic>
                <character n="Lucy">
                <soundeffect>HAHAHAHAHA!!!</soundeffect>
                </character>
                <character n="Charlie Brown">
                    <dialogue>?</dialogue>
                </character>
            </graphic>
        </panel>
        <panel n="4">
            <graphic>
                <character n="Lucy">
                    <dialogue>OH MY CHARLIE BROWN! YOU MAY NOT BE THE <bold>BRIGHTEST</bold> PERSON IN THE WORLD, BUT YOU SURE HAVE SOME <bold>IMAGINATION</bold>!</dialogue>
                </character>
                <character n="Charlie Brown">
                    <dialogue>good grief!</dialogue>
                </character>
                <signature/>
            </graphic>
        </panel>
        <footer>
            <link>https://www.gocomics.com/peanuts/1957/05/11</link>
        </footer>
    </comic>
    

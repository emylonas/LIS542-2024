    <comic n="4">
        <header><title>Spiderman</title> by <creator n="1">Stan Lee</creator> and <creator n="2">Larry Lieber</creator>.<date when="1980-11-30">November 30, 1980</date></header>
        <panel n="1">
            <character n="bank teller">
                <dialogue>
                    ANY <bold>IDENTIFICATION?</bold>
                </dialogue>
                <dialogue>
                    <bold>ANYONE</bold> CAN RENT A COSTUME!
                </dialogue>
            </character>
            <character n="Spiderman/Peter Parker">
                <dialogue>
                    LOOK AT MY <bold>COSTUME!</bold> WHO DO YOU <bold>THINK</bold> I AM?!!
                </dialogue>
            </character>
            <foreground n="white box">
                <creator-signature>
                    STAN LEE - LARRY LIEBER <date when="11-30">11-30</date>
                </creator-signature>
            </foreground>
        </panel>
        <panel n="2">
            <foreground>
                <character n="green hat/orange shirt"/>
                <character n="ginger hair/black jacket"/>
            </foreground>
            <background n="bank">
                <character n="bank teller">
                    <dialogue>
                        DRIVER'S LISCENSE? SOCIAL SECURITY?
                    </dialogue>
                </character>
                <character n="Spiderman/Peter Parker">
                    <dialogue>
                        <bold>LOOK!</bold> I'VE A <bold>SECRET IDENTITY!</bold> I CAN'T BLOW MY <bold>COVER!</bold>
                    </dialogue>
                </character>
            </background>
        </panel>
        <panel n="3">
            <foreground>
                <character n="bank teller">
                    <dialogue>
                        AND <bold>WE</bold> CAN'T CASH JUST <bold>ANY</bold> OLD CHECK!     
                    </dialogue>
                    <dialogue>
                        AND YOU'RE HOLDING UP THE <bold>LINE!</bold>
                    </dialogue>
                    <item n="check"/>
                </character>
                <character n="Spiderman/Peter Parker"/>
            </foreground>
            <background>
                <character n="black haired woman"/>
                <character n="green hat man"/>
                <character n="ginger man"/>
                <character n="blonde woman"/>
            </background>
        </panel>
        <panel n="4">
            <background n="cityscape"/>
            <midground>
                <character n="Spiderman/Peter Parker">
                    <dialogue>
                        I FINALLY GET $2,000-- AND <bold>I CAN'T CASH THE CHECK! O.K.!</bold> THERE'S ONLY <bold>ONE</bold> THING LEFT TO DO!
                    </dialogue>
                </character>
            </midground>
            <foreground n="yellow box">
                <text>
                    NEXT: <bold>THE BREAKING POINT!</bold>
                </text>
            </foreground> 
        </panel>

   <text>
      <body>
         
         <head><emph>Peanuts</emph>, <date when="1986-04-26">26 April 1986</date></head>
         
         <div type="panelGrp" xml:id="Peanuts1986-04-26">
            
            <cbml:panel n="1" characters="Linus">
               <note resp="#r-AP" type="panelDesc">Linus kneels on the ground facing a Jack-in-the-Box.</note>
               <docDate>4-26</docDate>               
            </cbml:panel>
            
            <cbml:panel n="2" characters="Linus" ana="#"><!-- In panels 2, 3, and 4, also record the transition type according to McCloud using the tr- value from the taxonomy -->
               <note resp="#r-AP" type="panelDesc">Linus winds up the Jack-in-the-Box.</note>
               <cbml:figure>Music Notes</cbml:figure>
            </cbml:panel>
            
            <cbml:panel n="3" characters="Linus" ana="#">
               <note resp="#r-AP" type="panelDesc">Linus gazes lovingly at the Jack-in-the-Box puppet as it springs from the box.</note>
               <cbml:figure>heart</cbml:figure>
            </cbml:panel>
            
            <cbml:panel n="4" characters="Linus" ana="#">
               <note resp="#r-AP" type="panelDesc">Linus remains kneeling in front of and looking at the Jack-in-the-Box while speaking to it and himself.</note>
               <cbml:balloon type="speech" who="Linus">I fall in love too easy...</cbml:balloon>
               <docAuthor>SCHULZ</docAuthor>
            </cbml:panel>
            
         </div>

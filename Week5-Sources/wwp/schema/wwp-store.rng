            <value>properAdjective</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
            <value>regularized</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
            <value>titlePage</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
            <value>person-female</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
            <value>person</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
            <value>person-male</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> </a:documentation>
          </choice>
        </attribute>
      </optional>
      <empty/>
